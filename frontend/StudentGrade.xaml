<Window x:Class="frontend.StudentGrade"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:frontend"
        mc:Ignorable="d"
        Title="StudentGrade" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="59*"/>
            <RowDefinition Height="375*"/>
        </Grid.RowDefinitions>

        <Label Name="labelUsername" Content="Label" HorizontalAlignment="Left" Margin="60,31,0,0" VerticalAlignment="Top" Width="110"/>
        <DataGrid Name="gridMyGrades" Grid.Row="1" FontSize="22"/>

    </Grid>
</Window>
